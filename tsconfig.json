{
    "compilerOptions": {
        "target": "es6",
        "sourceMap": true,
        "allowJs": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "types": ["node", "enonic-types"]
    },
    "include": ["src/main/resources/**/*.ts"]
}
