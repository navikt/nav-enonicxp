<?xml version="1.0" encoding="UTF-8"?>
<site>
    <x-data name="cmsContent" optional="true"/>
    <x-data name="cmsMenu" optional="true"/>
    <x-data name="cmsStatus" optional="true"/>
    <x-data name="menu-item" optional="true"/>
    <x-data name="fasetter" optional="true"/>
    <x-data name="oldContentType" optional="true"/>
    <mappings>
        <mapping controller="/lib/controllers/site-info.js">
            <match>type:'portal:site'</match>
        </mapping>
        <mapping controller="/lib/controllers/global-values-editor.js">
            <match>type:'no.nav.navno:global-value-set'</match>
        </mapping>
        <mapping controller="/lib/controllers/admin-frontend-proxy.js">
            <pattern>/.*</pattern>
        </mapping>
    </mappings>
</site>
