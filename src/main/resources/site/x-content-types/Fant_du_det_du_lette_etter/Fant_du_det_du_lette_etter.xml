<content-type>
    <display-name>Fant_du_det_du_lette_etter</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Overskrift">
            <label>Overskrift</label>
            <items>
                <input name="title" type="TextLine">
                    <label>Tittel</label>
                    <custom-text>contentdata/title</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="Radiobokser">
            <label>Radiobokser</label>
            <items>
                <input name="yes" type="TextLine">
                    <label>Positivt svar</label>
                    <custom-text>contentdata/yes</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="no" type="TextLine">
                    <label>Negativt svar</label>
                    <custom-text>contentdata/no</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="Beskrivelse">
            <label>Beskrivelse</label>
            <items>
                <input name="textinput" type="TextLine">
                    <label>Fritekstspørsmål</label>
                    <custom-text>contentdata/textinput</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="charactersleft" type="TextLine">
                    <label>[Antall] tegn igjen</label>
                    <custom-text>contentdata/charactersleft</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="Innsending">
            <label>Innsending</label>
            <items>
                <input name="button" type="TextLine">
                    <label>Tekst på knapp</label>
                    <custom-text>contentdata/button</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="Takk">
            <label>Takk</label>
            <items>
                <input name="thanks" type="TextLine">
                    <label>Tilbakemelding etter innsending</label>
                    <custom-text>contentdata/thanks</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="Feilmelding">
            <label>Feilmelding</label>
            <items>
                <input name="form-feilmelding" type="TextLine">
                    <label>Feilmelding ved å ikke ha valgt ja/nei</label>
                    <custom-text>contentdata/form-feilmelding</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="PlaceholderTekst">
            <label>PlaceholderTekst</label>
            <items>
                <input name="placeholder" type="TextLine">
                    <label>Placeholder for fritekstfelt</label>
                    <custom-text>contentdata/placeholder</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="KontaktOssTekst">
            <label>KontaktOssTekst</label>
            <items>
                <input name="kontaktoss" type="HtmlArea">
                    <label>Veiledende tekst under fritekstfelt</label>
                    <custom-text>contentdata/kontaktoss</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
