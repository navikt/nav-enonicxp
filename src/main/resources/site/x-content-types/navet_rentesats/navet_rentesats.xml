<content-type>
    <display-name>navet_rentesats</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Rentesats">
            <label>Rentesats</label>
            <items>
                <input name="periodestart" type="Date">
                    <label>Periode start</label>
                    <custom-text>contentdata/periodestart</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="periodeslutt" type="Date">
                    <label>Periode slutt</label>
                    <custom-text>contentdata/periodeslutt</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="rentesats" type="TextLine">
                    <label>Rentesats (bruk punktum som skilletegn, eks: 7.75)</label>
                    <custom-text>contentdata/rentesats</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
