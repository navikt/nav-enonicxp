<content-type>
    <display-name>Rettskildene_trygderettkjennels</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Kjennelse">
            <label>Kjennelse</label>
            <items>
                <input name="nativeId" type="TextLine">
                    <label>nativeId</label>
                    <custom-text>contentdata/nativeId</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="lopenummer" type="TextLine">
                    <label>Løpenummer</label>
                    <custom-text>contentdata/lopenummer</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="title" type="TextLine">
                    <label>Tittel</label>
                    <custom-text>contentdata/title</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="lovhjemmel" type="TextLine">
                    <label>Lovhjemler</label>
                    <custom-text>contentdata/lovhjemmel</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="year" type="TextLine">
                    <label>År</label>
                    <custom-text>contentdata/year</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="content" type="HtmlArea">
                    <label>Innhold</label>
                    <custom-text>contentdata/content</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
