                        <option value="Ja">Ja</option>
                        <option value="Nei">Nei</option>
                    </config>
                </input>
                <input name="url" type="TextLine">
                    <label>Side</label>
                    <custom-text>contentdata/url</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="language" type="TextLine">
                    <label>SprÃ¥k</label>
                    <custom-text>contentdata/language</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="description" type="TextArea">
                    <label>Tilbakemelding</label>
                    <custom-text>contentdata/description</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
