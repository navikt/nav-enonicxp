<content-type>
    <display-name>Skjemaveileder_hjelpetekster</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Skjematekster">
            <label>Skjematekster</label>
            <items>
                <input name="heading" type="TextLine">
                    <label>Skjemaoverskrift</label>
                    <custom-text>contentdata/heading</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="Generelle tekster">
            <label>Generelle tekster</label>
            <items>
                <input name="zip" type="TextLine">
                    <label>Postnummertekst</label>
                    <custom-text>contentdata/zip</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="snr" type="TextLine">
                    <label>Fødselsnummertekst</label>
                    <custom-text>contentdata/snr</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="navoffice" type="TextLine">
                    <label>Ledetekst for valgt NAV-enhet</label>
                    <custom-text>contentdata/navoffice</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="coaddress" type="TextLine">
                    <label>C/o-adresse</label>
                    <custom-text>contentdata/coaddress</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="streetaddress" type="TextLine">
                    <label>Gateadresse</label>
                    <custom-text>contentdata/streetaddress</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="postalplace" type="TextLine">
                    <label>Poststed</label>
                    <custom-text>contentdata/postalplace</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="navunit" type="TextLine">
                    <label>Nav-enhet</label>
                    <custom-text>contentdata/navunit</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="navinternational" type="TextLine">
                    <label>NAV Internasjonalt</label>
                    <custom-text>contentdata/navinternational</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="navpensjon" type="TextLine">
                    <label>NAV Pensjon</label>
                    <custom-text>contentdata/navpensjon</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="Lokalkontor">
            <label>Lokalkontor</label>
            <items>
                <input name="localoffice" type="TextLine">
                    <label>Tekst for lokalkontor</label>
                    <custom-text>contentdata/localoffice</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="localofficehelptext" type="TextArea">
                    <label>Beskrivelse for lokalkontor</label>
                    <custom-text>contentdata/localofficehelptext</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="Feilmeldinger">
            <label>Feilmeldinger</label>
            <items>
                <input name="generalerror" type="TextLine">
                    <label>Generell feilmelding</label>
                    <custom-text>contentdata/generalerror</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="invalidcategory" type="TextLine">
                    <label>Du må velge en kategori</label>
                    <custom-text>contentdata/invalidcategory</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="invalidcoaddress" type="TextLine">
                    <label>Ugyldig coadresse</label>
                    <custom-text>contentdata/invalidcoaddress</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="invalidstreetaddress" type="TextLine">
                    <label>Ugyldig gateadresse</label>
                    <custom-text>contentdata/invalidstreetaddress</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="missingfnr" type="TextLine">
                    <label>Manglende fødselsnummer</label>
                    <custom-text>contentdata/missingfnr</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="invalidzip" type="TextLine">
                    <label>Ugyldig postnummer</label>
                    <custom-text>contentdata/invalidzip</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="noaddress" type="TextLine">
                    <label>Manglende adresse</label>
                    <custom-text>contentdata/noaddress</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="invalidfnr" type="TextLine">
                    <label>Ugyldig fødselsnummer</label>
                    <custom-text>contentdata/invalidfnr</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="noattachmentschosen" type="TextLine">
                    <label>Du har ikke valgt noen vedlegg</label>
                    <custom-text>contentdata/noattachmentschosen</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="notchosenanysituation" type="TextLine">
                    <label>Du har ikke valgt bosituasjon</label>
                    <custom-text>contentdata/notchosenanysituation</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="navunitmultipleerror" type="TextLine">
                    <label>Du har ikke valgt NAV-enhet (på vegne av flere)</label>
                    <custom-text>contentdata/navunitmultipleerror</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="navunitorgerror" type="TextLine">
                    <label>Du har ikke valgt NAV-enhet (organisasjon)</label>
                    <custom-text>contentdata/navunitorgerror</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="registeredinnorwayornot" type="TextLine">
                    <label>Du må svare på om du er folkeregistrert i Norge eller ikke.</label>
                    <custom-text>contentdata/registeredinnorwayornot</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="frontpageerrormsg" type="TextLine">
                    <label>Du må svare på om du allerede har førstesiden eller ikke</label>
                    <custom-text>contentdata/frontpageerrormsg</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
