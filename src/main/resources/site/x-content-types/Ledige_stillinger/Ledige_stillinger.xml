<content-type>
    <display-name>Ledige_stillinger</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Ledig stilling">
            <label>Ledig stilling</label>
            <items>
                <input name="stilling" type="TextLine">
                    <label>Stilling</label>
                    <custom-text>contentdata/stilling</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="contactperson" type="TextLine">
                    <label>Kontaktperson</label>
                    <custom-text>contentdata/contactperson</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="contactperson_email" type="TextLine">
                    <label>E-post for kontaktperson</label>
                    <custom-text>contentdata/contactperson_email</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="description" type="TextArea">
                    <label>Kort beskrivelse av stillingen</label>
                    <custom-text>contentdata/description</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="antall" type="TextLine">
                    <label>Antall</label>
                    <custom-text>contentdata/antall</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="arbeidssted" type="TextLine">
                    <label>Arbeidssted</label>
                    <custom-text>contentdata/arbeidssted</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="fylke" type="ComboBox">
                    <label>Fylke</label>
                    <custom-text>contentdata/fylke</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <option value="Akershus">Akershus</option>
                        <option value="Aust-Agder">Aust-Agder</option>
                        <option value="Buskerud">Buskerud</option>
                        <option value="Finnmark">Finnmark</option>
                        <option value="Hedmark">Hedmark</option>
                        <option value="Hordaland">Hordaland</option>
                        <option value="Møre og Romsdal">Møre og Romsdal</option>
                        <option value="Nordland">Nordland</option>
                        <option value="Nord-Trøndelag">Nord-Trøndelag</option>
                        <option value="Oppland">Oppland</option>
                        <option value="Oslo">Oslo</option>
                        <option value="Rogaland">Rogaland</option>
                        <option value="Sogn og Fjordane">Sogn og Fjordane</option>
                        <option value="Sør-Trøndelag">Sør-Trøndelag</option>
                        <option value="Telemark">Telemark</option>
                        <option value="Troms">Troms</option>
                        <option value="Vest-Agder">Vest-Agder</option>
                        <option value="Vestfold">Vestfold</option>
                        <option value="Østfold">Østfold</option>
                        <option value="Svalbard">Svalbard</option>
                    </config>
                </input>
                <input name="registreringsdato" type="Date">
                    <label>Reg.dato</label>
                    <custom-text>contentdata/registreringsdato</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="soknadsfrist" type="Date">
                    <label>Søknadsfrist</label>
                    <custom-text>contentdata/soknadsfrist</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="internekstern" type="RadioButton">
                    <label>Intern/ekstern</label>
                    <custom-text>contentdata/internekstern</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                    <config>
                        <option value="Intern">Intern</option>
                        <option value="Ekstern">Ekstern</option>
                    </config>
                </input>
                <input name="relatedlink" type="ContentSelector">
                    <label>Utlysingstekst</label>
                    <custom-text>contentdata/relatedlink</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Fil</allow-content-type>
                    </config>
                </input>
                <input name="url" type="TextLine">
                    <label>Utlysingstekst-URL</label>
                    <custom-text>contentdata/url</custom-text>
                    <help-text>Bruk fullstendig URL, f.eks. http://www.nav.no/.</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
