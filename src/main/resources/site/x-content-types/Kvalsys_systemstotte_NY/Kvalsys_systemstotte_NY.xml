<content-type>
    <display-name>Kvalsys_systemstotte_NY</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Om systemstøtten">
            <label>Om systemstøtten</label>
            <items>
                <input name="heading" type="TextLine">
                    <label>Tittel</label>
                    <custom-text>contentdata/heading</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <item-set name="listitem">
            <label>Bilde</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="bilde" type="ImageSelector">
                    <label>Bilde</label>
                    <custom-text>image</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="bildetekst" type="TextArea">
                    <label>Bildetekst</label>
                    <custom-text>text</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </item-set>
    </form>
</content-type>
