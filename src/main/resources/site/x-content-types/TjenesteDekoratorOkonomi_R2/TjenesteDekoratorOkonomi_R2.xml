<content-type>
    <display-name>TjenesteDekoratorOkonomi_R2</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="applicationUrlPath">
            <label>applicationUrlPath</label>
            <items>
                <input name="tjeneste" type="TextLine">
                    <label>Tjeneste</label>
                    <custom-text>contentdata/tjeneste</custom-text>
                    <help-text>Denne teksten brukes for å beskrive aktuell tjeneste</help-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="toplevel" type="TextLine">
                    <label>Toppnivåmenypunkt</label>
                    <custom-text>contentdata/toplevel</custom-text>
                    <help-text>Dette menypunktet brukes for å identifisere konfigurasjonen</help-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <item-set name="decorate">
            <label>nodecorate</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="performdecorationpattern" type="TextLine">
                    <label>Mønster</label>
                    <custom-text>performdecorationpattern</custom-text>
                    <help-text>Sider som matcher dette mønsteret vil ikke bli dekorert.</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </item-set>
        <item-set name="title">
            <label>decoratorTitle</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="decoratorTitle">
                    <label>decoratorTitle</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="title_pattern" type="TextLine">
                            <label>Pattern</label>
                            <custom-text>decoratorTitlePattern</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="title_text" type="TextLine">
                            <label>Title</label>
                            <custom-text>decoratorTitleText</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
        <item-set name="headerbars">
            <label>headerbar</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="headerbar">
                    <label>headerbar</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="insertheaderbarcomponentpattern" type="TextLine">
                            <label>Mønster</label>
                            <custom-text>insertheaderbarcomponentpattern</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
        <item-set name="fjerntilgjengelighetslinjer">
            <label>fjerntilgjengelighetslinje</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="fjerntilgjengelighetslinje">
                    <label>fjerntilgjengelighetslinje</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="fjerntilgjengelighetslinjepattern" type="TextLine">
                            <label>Mønster</label>
                            <custom-text>fjerntilgjengelighetslinjepattern</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
        <item-set name="disablestatisticsscripts">
            <label>disablestatisticsscripts</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="disablestatisticsscripts">
                    <label>disablestatisticsscripts</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="disablestatisticsscriptspattern" type="TextLine">
                            <label>Mønster</label>
                            <custom-text>disablestatisticsscriptspattern</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
        <item-set name="useasyncgooglescripts">
            <label>useasyncgooglescript</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="useasyncgooglescript">
                    <label>useasyncgooglescript</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="useasyncgooglescriptpattern" type="TextLine">
                            <label>Mønster</label>
                            <custom-text>useasyncgooglescriptpattern</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
        <item-set name="disableinnlogginslinjer">
            <label>disableinnloggingslinje</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="disableinnlogginslinje">
                    <label>disableinnloggingslinje</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="disableinnlogginslinjepattern" type="TextLine">
                            <label>Mønster</label>
                            <custom-text>disableinnlogginslinjepattern</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
        <item-set name="scores">
            <label>cp</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="cp">
                    <label>cp</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="scores_cp_pattern" type="TextLine">
                            <label>Pattern</label>
                            <custom-text>scores_cp_pattern</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="scores_cp_value" type="TextLine">
                            <label>Verdi</label>
                            <custom-text>scores_cp_value</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
        <item-set name="javascript">
            <label>disableFellesJS</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="disableFellesJS" type="CheckBox">
                    <label>Ekskluder felles javascript</label>
                    <custom-text>disable_fellesJS</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </item-set>
    </form>
</content-type>
