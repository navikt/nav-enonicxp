<content-type>
    <display-name>veivisersosialhjelp_lenkeboks</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Lenkeboks">
            <label>Lenkeboks</label>
            <items>
                <input name="tittel" type="TextLine">
                    <label>Tittel</label>
                    <custom-text>contentdata/tittel</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="id" type="TextLine">
                    <label>Rekkef√∏lge</label>
                    <custom-text>contentdata/id</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="lenker" type="ContentSelector">
                    <label>Lenker</label>
                    <custom-text>contentdata/lenker</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Ekstern_lenke</allow-content-type>
                        <allow-content-type>no.nav.navno:Artikkel_Brukerportal</allow-content-type>
                        <allow-content-type>no.nav.navno:Kort_om</allow-content-type>
                        <allow-content-type>no.nav.navno:veivisersosialhjelp_artikler</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
