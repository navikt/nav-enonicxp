<content-type>
    <display-name>Nokkeltall</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Tabell">
            <label>Tabell</label>
            <items>
                <input name="title" type="TextLine">
                    <label>Nøkkeltalloverskrift</label>
                    <custom-text>contentdata/title</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="organisation" type="ContentSelector">
                    <label>Enhet</label>
                    <custom-text>contentdata/organisation</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Enhetsinformasjon</allow-content-type>
                    </config>
                </input>
                <input name="dictionary" type="ContentSelector">
                    <label>Emneord</label>
                    <custom-text>contentdata/dictionary</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Emne</allow-content-type>
                    </config>
                </input>
                <input name="language" type="ContentSelector">
                    <label>Språk</label>
                    <custom-text>contentdata/language</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Sprak</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <item-set name="keynumbers">
            <label>Nokkeltall</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="keynumber">
                    <label>Nokkeltall</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="ledetekst" type="TextLine">
                            <label>Ledetekst til nøkkeltall</label>
                            <custom-text>text</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="number" type="TextLine">
                            <label>Nøkkeltall</label>
                            <custom-text>number</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
    </form>
</content-type>
