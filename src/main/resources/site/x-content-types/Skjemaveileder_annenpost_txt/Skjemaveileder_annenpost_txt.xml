<content-type>
    <display-name>Skjemaveileder_annenpost_txt</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Skjemaoverskrift">
            <label>Skjemaoverskrift</label>
            <items>
                <input name="heading" type="TextLine">
                    <label>Skjemaoverskrift</label>
                    <custom-text>contentdata/heading</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="headingexplained" type="TextArea">
                    <label>Forklaring</label>
                    <custom-text>contentdata/headingexplained</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Velg tema">
            <label>Velg tema</label>
            <items>
                <input name="categoryheadline" type="TextLine">
                    <label>Velg tema-overskrift</label>
                    <custom-text>contentdata/categoryheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="categoryselect" type="TextLine">
                    <label>Rullegardinmenytekst til Velg tema</label>
                    <custom-text>contentdata/categoryselect</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="categoryfactsheetlink" type="TextLine">
                    <label>Lenke til faktaark</label>
                    <custom-text>contentdata/categoryfactsheetlink</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="categoryfactsheettext" type="TextLine">
                    <label>Lenketekst til faktaark</label>
                    <custom-text>contentdata/categoryfactsheettext</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Velg avsender">
            <label>Velg avsender</label>
            <items>
                <input name="senderheadline" type="TextLine">
                    <label>Velg avsender: Overskrift</label>
                    <custom-text>contentdata/senderheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="På vegne av en selv eller annen person">
            <label>På vegne av en selv eller annen person</label>
            <items>
                <input name="senderinnorwayheadline" type="TextLine">
                    <label>På vegne av en selv eller annen person, overskrift</label>
                    <custom-text>contentdata/senderinnorwayheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="senderinnorwaydescription" type="TextArea">
                    <label>På vegne av en selv eller annen person, beskrivelse</label>
                    <custom-text>contentdata/senderinnorwaydescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Folkeregistrert og bor i Norge">
            <label>Folkeregistrert og bor i Norge</label>
            <items>
                <input name="innorwayheadline" type="TextLine">
                    <label>Folkeregistert og bor i Norge, overskrift</label>
                    <custom-text>contentdata/innorwayheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="innorwaydescription" type="TextArea">
                    <label>Folkeregistert og bor i Norge, beskrivelse</label>
                    <custom-text>contentdata/innorwaydescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="innorwayzip" type="TextLine">
                    <label>Ledetekst for postnr</label>
                    <custom-text>contentdata/innorwayzip</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="innorwayfnr" type="TextLine">
                    <label>Ledetekst for fødselsnummer</label>
                    <custom-text>contentdata/innorwayfnr</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="innorwayzipdescription" type="TextLine">
                    <label>Beskrivelse for postnr</label>
                    <custom-text>contentdata/innorwayzipdescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="innorwayfnrdescription" type="TextArea">
                    <label>Beskrivelse for fødselsnummer</label>
                    <custom-text>contentdata/innorwayfnrdescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Ikke folkeregistrert i Norge">
            <label>Ikke folkeregistrert i Norge</label>
            <items>
                <input name="notinnorwayheadline" type="TextLine">
                    <label>Ikke folkeregistrert i Norge, overskrift</label>
                    <custom-text>contentdata/notinnorwayheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notinnorwaydescription" type="TextArea">
                    <label>Ikke folkeregistert i Norge, beskrivelse</label>
                    <custom-text>contentdata/notinnorwaydescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notinnorwayaddress" type="TextLine">
                    <label>Ledetekst for adresse</label>
                    <custom-text>contentdata/notinnorwayaddress</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notinnorwaycoaddress" type="TextLine">
                    <label>Coadresse</label>
                    <custom-text>contentdata/notinnorwaycoaddress</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notinnorwaystreetaddress" type="TextLine">
                    <label>Gateadresse</label>
                    <custom-text>contentdata/notinnorwaystreetaddress</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notinnorwayzip" type="TextLine">
                    <label>Postnummer</label>
                    <custom-text>contentdata/notinnorwayzip</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notinnorwaypostalplace" type="TextLine">
                    <label>Poststed</label>
                    <custom-text>contentdata/notinnorwaypostalplace</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notinnorwayaddressdescription" type="TextArea">
                    <label>Beskrivelse til feltene for adresse</label>
                    <custom-text>contentdata/notinnorwayaddressdescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Verken bor eller arbeider i Norge">
            <label>Verken bor eller arbeider i Norge</label>
            <items>
                <input name="notworkinnorwayheadline" type="TextLine">
                    <label>Verken bor eller arbeider i Norge, overskrift</label>
                    <custom-text>contentdata/notworkinnorwayheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notworkinnorwaydescription" type="TextArea">
                    <label>Verken bor eller arbeider i Norge, beskrivelse</label>
                    <custom-text>contentdata/notworkinnorwaydescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notworkinnorwayfnr" type="TextLine">
                    <label>Ledetekst for fødselsnummer</label>
                    <custom-text>contentdata/notworkinnorwayfnr</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="notworkinnorwayfnrdescription" type="TextArea">
                    <label>Beskrivelse for fødselsnummer</label>
                    <custom-text>contentdata/notworkinnorwayfnrdescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Adressesteg for NAV Internasjonalt">
            <label>Adressesteg for NAV Internasjonalt</label>
            <items>
                <input name="navinternasjonaltheadline" type="TextLine">
                    <label>NAV Internasjonalt, overskrift</label>
                    <custom-text>contentdata/navinternasjonaltheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="navinternasjonaltdescription" type="TextArea">
                    <label>NAV Internasjonalt, beskrivelse</label>
                    <custom-text>contentdata/navinternasjonaltdescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="navinternasjonalt" type="ContentSelector">
                    <label>Enhetsinformasjon NAV Internasjonalt (for riktig ID til NAV Internasjonalt)</label>
                    <custom-text>contentdata/navinternasjonalt</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Enhetsinformasjon</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <field-set name="Adressesteg for NAV Pensjon">
            <label>Adressesteg for NAV Pensjon</label>
            <items>
                <input name="navpensjonheadline" type="TextLine">
                    <label>NAV Pensjon, overskrift</label>
                    <custom-text>contentdata/navpensjonheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="navpensjondescription" type="TextArea">
                    <label>NAV Pensjon, beskrivelse</label>
                    <custom-text>contentdata/navpensjondescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="navpensjon" type="ContentSelector">
                    <label>Enhetsinformasjon NAV Pensjon (for riktig ID til NAV Pensjon)</label>
                    <custom-text>contentdata/navpensjon</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Enhetsinformasjon</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <field-set name="På vegne av flere brukere samtidig">
            <label>På vegne av flere brukere samtidig</label>
            <items>
                <input name="multiplesenderheadline" type="TextLine">
                    <label>På vegne av flere brukere samtidig, overskrift</label>
                    <custom-text>contentdata/multiplesenderheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="multiplesenderdescription" type="TextArea">
                    <label>På vegne av flere brukere samtidig, beskrivelse</label>
                    <custom-text>contentdata/multiplesenderdescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="multiplesendernavunit" type="TextLine">
                    <label>Ledetekst for NAV-enhet</label>
                    <custom-text>contentdata/multiplesendernavunit</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="multiplesendernavunitdescription" type="TextArea">
                    <label>Forklaring for inputboks for NAV-enhet</label>
                    <custom-text>contentdata/multiplesendernavunitdescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="På vegne av en organisasjon">
            <label>På vegne av en organisasjon</label>
            <items>
                <input name="organizationheadline" type="TextLine">
                    <label>På vegne av en organisasjon, overskrift</label>
                    <custom-text>contentdata/organizationheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="organizationdescription" type="TextArea">
                    <label>På vegne av en organisasjon, beskrivelse</label>
                    <custom-text>contentdata/organizationdescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="organizationnavunit" type="TextLine">
                    <label>Ledetekst for NAV-enhet</label>
                    <custom-text>contentdata/organizationnavunit</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="organizationnavunitdescription" type="TextArea">
                    <label>Forklaring for inputboks for NAV-enhet</label>
                    <custom-text>contentdata/organizationnavunitdescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Last ned førsteside">
            <label>Last ned førsteside</label>
            <items>
                <input name="showfirstpageheadline" type="TextLine">
                    <label>Last ned førsteside: Overskrift</label>
                    <custom-text>contentdata/showfirstpageheadline</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="showfirstpagedescription" type="TextArea">
                    <label>Last ned førsteside: Beskrivelse</label>
                    <custom-text>contentdata/showfirstpagedescription</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="showfirstpagedownload" type="TextLine">
                    <label>Lenketekst til nedlasting</label>
                    <custom-text>contentdata/showfirstpagedownload</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="nextstep" type="TextLine">
                    <label>Neste steg</label>
                    <custom-text>contentdata/nextstep</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
