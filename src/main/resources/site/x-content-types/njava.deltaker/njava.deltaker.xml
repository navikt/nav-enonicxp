<content-type>
    <display-name>njava.deltaker</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Deltaker">
            <label>Deltaker</label>
            <items>
                <input name="first-name" type="TextLine">
                    <label>Fornavn</label>
                    <custom-text>contentdata/first-name</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="last-name" type="TextLine">
                    <label>Etternavn</label>
                    <custom-text>contentdata/last-name</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="email" type="TextLine">
                    <label>E-post</label>
                    <custom-text>contentdata/email</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
