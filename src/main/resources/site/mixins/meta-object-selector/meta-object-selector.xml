<mixin>
  <form>
    <input name="excludedContent" type="CustomSelector">
        <label>Velg meta-objekt som skal kobles</label>
        <occurrences minimum="0" maximum="0"/>
        <config>
            <service>contentSelector</service>
            <param value="contentTypes">
                ["no.nav.navno:page-meta-object"]
            </param>
            <param value="selectorQuery">
              data.contentType._selected = "{type}"
            </param>
        </config>
    </input>
  </form>
</mixin>