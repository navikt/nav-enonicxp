<?xml version="1.0" encoding="UTF-8"?>
<mixin>
  <display-name>CMS Menu Item</display-name>
  <items>
    <input type="TextLine" name="menuKey">
      <label>Menu item key</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input type="ContentSelector" name="content">
      <label>Menu item related content</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
  </items>
</mixin>