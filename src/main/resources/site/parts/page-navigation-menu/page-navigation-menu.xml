<part xmlns="urn:enonic:xp:model:1.0">
    <display-name>Meny for intern-navigasjon</display-name>
    <form>
        <input name="viewStyle" type="RadioButton">
            <label>Visningstype</label>
            <occurrences minimum="1" maximum="1" />
            <config>
                <option value="sidebar">Smal (sidemeny)</option>
                <option value="inContent">Bred (innholdsseksjon)</option>
            </config>
            <default>inContent</default>
        </input>
        <mixin name="page-navigation-menu"/>
    </form>
</part>
