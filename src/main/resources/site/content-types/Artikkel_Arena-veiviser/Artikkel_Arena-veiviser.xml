<content-type>
    <display-name>Artikkel_Arena-veiviser</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Artikkel">
            <label>Artikkel</label>
            <items>
                <input name="heading" type="TextLine">
                    <label>Tittel</label>
                    <custom-text>contentdata/heading</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="ingress" type="TextArea">
                    <label>Ingress</label>
                    <custom-text>contentdata/ingress</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <item-set name="videos">
            <label>Video</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="video">
                    <label>Video</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="videoname" type="TextLine">
                            <label>Filmnavn</label>
                            <custom-text>name</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
    </form>
</content-type>
