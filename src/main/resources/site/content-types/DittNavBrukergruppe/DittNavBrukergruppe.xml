<content-type>
    <display-name>DittNavBrukergruppe</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Brukergruppe">
            <label>Brukergruppe</label>
            <items>
                <input name="type" type="ComboBox">
                    <label>Navn</label>
                    <custom-text>contentdata/type</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                    <config>
                        <option value="Generell_bruker">GenerellBruker</option>
                        <option value="AAP">AAP</option>
                        <option value="Ingen_ytelse">Ingen_ytelse</option>
                        <option value="Individstonad">IndividstÃ¸nad</option>
                        <option value="Dagpenger">Dagpenger</option>
                        <option value="Postkasse">Postkasse</option>
                    </config>
                </input>
            </items>
        </field-set>
        <field-set name="Tjenester">
            <label>Tjenester</label>
            <items>
                <input name="tjeneste" type="ContentSelector">
                    <label>Tjeneste</label>
                    <custom-text>contentdata/tjeneste</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:DittNavTjeneste</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <field-set name="ViktigeTjenester">
            <label>ViktigeTjenester</label>
            <items>
                <input name="antallViktigeTjenester" type="ComboBox">
                    <label>Viktige tjenester</label>
                    <custom-text>contentdata/antallViktigeTjenester</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                    <config>
                        <option value="3">3</option>
                        <option value="6">6</option>
                        <option value="9">9</option>
                        <option value="12">12</option>
                    </config>
                </input>
            </items>
        </field-set>
        <item-set name="relatertinnhold">
            <label>Relatert innhold</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="artikkel" type="ContentSelector">
                    <label>Artikkel</label>
                    <custom-text>artikkel</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:DittNavArtikkel</allow-content-type>
                    </config>
                </input>
            </items>
        </item-set>
    </form>
</content-type>
