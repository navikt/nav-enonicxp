                                    <config>
                                        <service>contentSelector</service>
                                        <param value="contentTypes">no.nav.navno:product-page</param>
                                        <param value="selectorQuery">language!="no" AND language!="nn" AND language!="en" AND language!="se" AND data.product="{id}"</param>
                                    </config>
                                </input>
                            </items>
                        </field-set>
                        <field-set>
                            <label>Produktdetaljer</label>
                            <items>
