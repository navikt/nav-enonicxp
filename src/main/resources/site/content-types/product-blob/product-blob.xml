                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="lang" type="TextLine">
                            <label>To-bokstavers språkkode</label>
                            <occurrences maximum="1" minimum="1"/>
                            <config>
                                <max-length>2</max-length>
                                <regexp>^[a-zA-Z]{2}$</regexp>
