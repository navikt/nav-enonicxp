                    </items>
                </item-set>
                <item-set name="contentOther">
                    <label>Andre språk</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="lang" type="TextLine">
                            <label>To-bokstavers språkkode</label>
                            <occurrences maximum="1" minimum="1"/>
                            <config>
                                <max-length>2</max-length>
                                <regexp>^[a-zA-Z]{2}$</regexp>
                            </config>
                        </input>
                        <field-set>
                            <label>Produktnavn og ingress</label>
                            <items>
                                <input name="title" type="TextLine">
                                    <label>Produktnavn</label>
                                    <help-text>Vises på oversiktssider og produktkort</help-text>
                                    <occurrences maximum="1" minimum="1"/>
