<content-type>
    <display-name>Rettskildene_virkestoff</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Virkestoff">
            <label>Virkestoff</label>
            <items>
                <input name="nativeid" type="TextLine">
                    <label>Native id</label>
                    <custom-text>contentdata/nativeid</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="name" type="TextLine">
                    <label>Navn</label>
                    <custom-text>contentdata/name</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="content" type="HtmlArea">
                    <label>Innhold</label>
                    <custom-text>contentdata/content</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
