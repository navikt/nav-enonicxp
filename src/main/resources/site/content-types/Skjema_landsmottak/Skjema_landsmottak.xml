<content-type>
    <display-name>Skjema_landsmottak</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Landsmottak">
            <label>Landsmottak</label>
            <items>
                <input name="title" type="TextLine">
                    <label>Tittel</label>
                    <custom-text>contentdata/title</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="kategori" type="ContentSelector">
                    <label>Kategori</label>
                    <custom-text>contentdata/kategori</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Emne</allow-content-type>
                    </config>
                </input>
                <input name="mottak" type="ContentSelector">
                    <label>Skjemamottak</label>
                    <custom-text>contentdata/mottak</custom-text>
                    <help-text>Spesialenhet som tar i mot skjema for valgte kategorier. Dette gjelder kun skjema som ikke kan scannes:
                    </help-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Enhetsinformasjon</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
