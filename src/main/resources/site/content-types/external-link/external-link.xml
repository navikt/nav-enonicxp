<content-type>
    <display-name>Ekstern Lenke</display-name>
    <description>Lenker ut av nav.no</description>
    <super-type>base:unstructured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set>
            <label>Ekstern lenke</label>
            <items>
                <input name="description" type="TextArea">
                    <label>Ingress</label>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="url" type="TextLine">
                    <label>URL</label>
                    <help-text>Benytt fullstendig URL. Eksempelvis slik: https://skatteetaten.no</help-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <mixin name="type-of-redirect"/>
            </items>
        </field-set>
        <mixin name="keywords"/>
    </form>
</content-type>
