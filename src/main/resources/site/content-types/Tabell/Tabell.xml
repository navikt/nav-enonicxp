<content-type>
    <display-name>Tabell</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Tabell">
            <label>Tabell</label>
            <items>
                <input name="title" type="TextLine">
                    <label>Tabelloverskrift (norsk)</label>
                    <custom-text>contentdata/title</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="originaltitle" type="TextLine">
                    <label>Tabelloverskrift (originalspråk)</label>
                    <custom-text>contentdata/originaltitle</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="summary" type="TextArea">
                    <label>Sammendrag</label>
                    <custom-text>contentdata/summary</custom-text>
                    <help-text>Sammendrag av tabellens formål og struktur for ikke-seende brukere, for eksempel: 
					«Kontantstøttesatser per 01.01.06. Første kolonne viser timer per uke i barnehage. 
					Andre kolonne viser beløp per måned.».</help-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="ingress" type="TextArea">
                    <label>Ingress</label>
                    <custom-text>contentdata/ingress</custom-text>
                    <help-text>Denne ingressen vil vises over tabellen</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="dictionary" type="ContentSelector">
                    <label>Emneord</label>
                    <custom-text>contentdata/dictionary</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Emne</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <item-set name="header">
            <label>Overskriftsrad</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="cell1" type="TextLine">
                    <label>Kolonne 1</label>
                    <custom-text>contentdata/rows/header/cell1</custom-text>
                    <help-text>Bare kolonner med overskrift blir tatt med i tabellen.</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell2" type="TextLine">
                    <label>Kolonne 2</label>
                    <custom-text>contentdata/rows/header/cell2</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell3" type="TextLine">
                    <label>Kolonne 3</label>
                    <custom-text>contentdata/rows/header/cell3</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell4" type="TextLine">
                    <label>Kolonne 4</label>
                    <custom-text>contentdata/rows/header/cell4</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell5" type="TextLine">
                    <label>Kolonne 5</label>
                    <custom-text>contentdata/rows/header/cell5</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell6" type="TextLine">
                    <label>Kolonne 6</label>
                    <custom-text>contentdata/rows/header/cell6</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell7" type="TextLine">
                    <label>Kolonne 7</label>
                    <custom-text>contentdata/rows/header/cell7</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell8" type="TextLine">
                    <label>Kolonne 8</label>
                    <custom-text>contentdata/rows/header/cell8</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell9" type="TextLine">
                    <label>Kolonne 9</label>
                    <custom-text>contentdata/rows/header/cell9</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell10" type="TextLine">
                    <label>Kolonne 10</label>
                    <custom-text>contentdata/rows/header/cell10</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell11" type="TextLine">
                    <label>Kolonne 11</label>
                    <custom-text>contentdata/rows/header/cell11</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell12" type="TextLine">
                    <label>Kolonne 12</label>
                    <custom-text>contentdata/rows/header/cell12</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cell13" type="TextLine">
                    <label>Kolonne 13</label>
                    <custom-text>contentdata/rows/header/cell13</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </item-set>
        <item-set name="rows">
            <label>Tabellrad</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="row">
                    <label>Tabellrad</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="cell1" type="TextLine">
                            <label>Kolonne 1</label>
                            <custom-text>cell1</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell2" type="TextLine">
                            <label>Kolonne 2</label>
                            <custom-text>cell2</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell3" type="TextLine">
                            <label>Kolonne 3</label>
                            <custom-text>cell3</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell4" type="TextLine">
                            <label>Kolonne 4</label>
                            <custom-text>cell4</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell5" type="TextLine">
                            <label>Kolonne 5</label>
                            <custom-text>cell5</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell6" type="TextLine">
                            <label>Kolonne 6</label>
                            <custom-text>cell6</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell7" type="TextLine">
                            <label>Kolonne 7</label>
                            <custom-text>cell7</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell8" type="TextLine">
                            <label>Kolonne 8</label>
                            <custom-text>cell8</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell9" type="TextLine">
                            <label>Kolonne 9</label>
                            <custom-text>cell9</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell10" type="TextLine">
                            <label>Kolonne 10</label>
                            <custom-text>cell10</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell11" type="TextLine">
                            <label>Kolonne 11</label>
                            <custom-text>cell11</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell12" type="TextLine">
                            <label>Kolonne 12</label>
                            <custom-text>cell12</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="cell13" type="TextLine">
                            <label>Kolonne 13</label>
                            <custom-text>cell13</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
        <field-set name="Merknad">
            <label>Merknad</label>
            <items>
                <input name="fotnote" type="TextArea">
                    <label>Merknad</label>
                    <custom-text>contentdata/fotnote</custom-text>
                    <help-text>Denne skal kun brukes hvis noen av tabellelementene er merket med *</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <item-set name="links">
            <label>Relaterte lenker</label>
            <occurrences maximum="1" minimum="0"/>
            <items>
                <item-set name="link">
                    <label>Relaterte lenker</label>
                    <occurrences maximum="0" minimum="0"/>
                    <items>
                        <input name="description" type="TextLine">
                            <label>Lenketekst</label>
                            <custom-text>description</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                        </input>
                        <input name="enhet" type="ContentSelector">
                            <label>Relatert innhold</label>
                            <custom-text>contentdata/enhet</custom-text>
                            <help-text>Legg til bare ett element i hver blokk. Kun den første lenken i hver blokk vil vises på siden.</help-text>
                            <occurrences maximum="0" minimum="0"/>
                            <config>
                                <allow-content-type>no.nav.navno:Fil</allow-content-type>
                                <allow-content-type>no.nav.navno:Nyhet_Brukerportal</allow-content-type>
                                <allow-content-type>no.nav.navno:Ekstern_lenke</allow-content-type>
                            </config>
                        </input>
                        <input name="destination" type="ComboBox">
                            <label>Åpne i</label>
                            <custom-text>target</custom-text>
                            <occurrences maximum="1" minimum="0"/>
                            <config>
                                <option value="current">Samme vindu</option>
                                <option value="new">Nytt vindu</option>
                            </config>
                        </input>
                    </items>
                </item-set>
            </items>
        </item-set>
    </form>
</content-type>
