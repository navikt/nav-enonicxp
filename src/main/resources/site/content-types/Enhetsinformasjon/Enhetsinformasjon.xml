<content-type>
    <display-name>Enhetsinformasjon</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Enhet">
            <label>Enhet</label>
            <items>
                <input name="type" type="ContentSelector">
                    <label>Type enhet</label>
                    <custom-text>contentdata/type</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                    <config>
                        <allow-content-type>no.nav.navno:Enhetstype</allow-content-type>
                    </config>
                </input>
                <input name="officeNumber" type="TextLine">
                    <label>Kontornummer</label>
                    <custom-text>contentdata/officeNumber</custom-text>
                    <help-text>NORG-nummer, fire siffer. Ikke Arena-nummer.</help-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="office" type="TextLine">
                    <label>Kontor</label>
                    <custom-text>contentdata/office</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="postaddress" type="TextLine">
                    <label>Postadresse</label>
                    <custom-text>contentdata/postaddress</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="postalcode" type="TextLine">
                    <label>Postnummer</label>
                    <custom-text>contentdata/postalcode</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="postalplace" type="TextLine">
                    <label>Poststed</label>
                    <custom-text>contentdata/postalplace</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="contry" type="TextLine">
                    <label>Land</label>
                    <custom-text>contentdata/contry</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="county" type="TextLine">
                    <label>Fylke</label>
                    <custom-text>contentdata/county</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="kommune" type="TextLine">
                    <label>Kommune</label>
                    <custom-text>contentdata/kommune</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="covers" type="TextLine">
                    <label>Dekker</label>
                    <custom-text>contentdata/covers</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="underlagt" type="TextLine">
                    <label>Underlagt</label>
                    <custom-text>contentdata/underlagt</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="organizationNumber" type="TextLine">
                    <label>Organisasjonsnummer</label>
                    <custom-text>contentdata/organizationNumber</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="forvaltningsenhet" type="ContentSelector">
                    <label>Forvaltningsenhet</label>
                    <custom-text>contentdata/forvaltningsenhet</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Enhetsinformasjon</allow-content-type>
                    </config>
                </input>
                <input name="visitaddress" type="TextLine">
                    <label>Besøksadresse</label>
                    <custom-text>contentdata/visitaddress</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="latlng" type="TextLine">
                    <label>Lengde- og breddegrader for Google Maps</label>
                    <custom-text>contentdata/latlng</custom-text>
                    <help-text>Tjenesten www.gps-coordinates.net gir lengde- og breddegrader. Nøyaktig plassering gis i formatet "59.928603, 10.7588177" (uten anførselstegn rundt).</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="telephone" type="TextLine">
                    <label>Telefon</label>
                    <custom-text>contentdata/telephone</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="telefax" type="TextLine">
                    <label>Telefaks</label>
                    <custom-text>contentdata/telefax</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="textphone" type="TextLine">
                    <label>Teksttelefon</label>
                    <custom-text>contentdata/textphone</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="interpretationService" type="TextLine">
                    <label>Tolketjeneste</label>
                    <custom-text>contentdata/interpretationService</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="openingHours" type="TextLine">
                    <label>Åpningstider</label>
                    <custom-text>contentdata/openingHours</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="generalManager" type="TextLine">
                    <label>Daglig leder</label>
                    <custom-text>contentdata/generalManager</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="title" type="TextLine">
                    <label>Tittel</label>
                    <custom-text>contentdata/title</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="information" type="TextArea">
                    <label>Opplysninger</label>
                    <custom-text>contentdata/information</custom-text>
                    <help-text>Bruk enten dette feltet eller «Opplysninger i avsnitt» nedenfor.</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="informationParagraphs" type="HtmlArea">
                    <label>Opplysninger i avsnitt</label>
                    <custom-text>contentdata/informationParagraphs</custom-text>
                    <help-text>Bruk enten dette feltet eller «Opplysninger» ovenfor.</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="kontorid" type="TextLine">
                    <label>kontorid</label>
                    <custom-text>contentdata/kontorid</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="E-post">
            <label>E-post</label>
            <items>
                <input name="email" type="TextLine">
                    <label>E-post ekstern</label>
                    <custom-text>contentdata/email</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="emailIntern" type="TextLine">
                    <label>E-post intern</label>
                    <custom-text>contentdata/emailIntern</custom-text>
                    <help-text>
            Bruk en adresse på formatet nav.kontor@nav.no, eventuelt kontor@aetat.no eller kontor@trygdeetaten.no, som helst ikke leder til kontaktsenteret.
            
            Denne brukes til serviceklager og ros.
          </help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Visning av e-postadresse eller 'Skriv til oss'-knapp på nav no">
            <label>Visning av e-postadresse eller 'Skriv til oss'-knapp på nav.no</label>
            <items>
                <input name="email-options" type="RadioButton">
                    <label>På nav.no:</label>
                    <custom-text>contentdata/email-options</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <option value="replace-email">Vis "skriv til oss"-knapp</option>
                        <option value="show-email">Vis e-postadresse</option>
                        <option value="hide-email">Vis verken e-postadresse eller lenke</option>
                    </config>
                </input>
            </items>
        </field-set>
        <item-set name="ringenettverk">
            <label>Ringenettverk</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="ringenettverkOmrade" type="TextLine">
                    <label>Område</label>
                    <custom-text>ringenettverkOmrade</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="ringenettverkTelephone" type="TextLine">
                    <label>Telefon</label>
                    <custom-text>ringenettverkTelephone</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </item-set>
        <item-set name="forms">
            <label>Skjemamottak</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="category" type="ContentSelector">
                    <label>Kategori</label>
                    <custom-text>category</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Emne</allow-content-type>
                    </config>
                </input>
                <input name="formoffice" type="ContentSelector">
                    <label>Skjemamottak</label>
                    <custom-text>formoffice</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Enhetsinformasjon</allow-content-type>
                    </config>
                </input>
            </items>
        </item-set>
    </form>
</content-type>
