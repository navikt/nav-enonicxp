<content-type>
    <display-name>Kvalsys_utlistingtitler_NY</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Oppdaterer du på vegne av noen andre?">
            <label>Oppdaterer du på vegne av noen andre?</label>
            <items>
                <input name="updated-by" type="TextLine">
                    <label>Oppdatert på vegne av</label>
                    <custom-text>contentdata/updated-by</custom-text>
                    <help-text>La feltet være tomt med mindre du redigerer innholdet på vegne av noen andre.</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Om utlisting av titler">
            <label>Om utlisting av titler</label>
            <items>
                <input name="heading" type="TextLine">
                    <label>Tittel på utlistingsgruppen</label>
                    <custom-text>contentdata/heading</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <field-set name="Første bolk være åpen?">
            <label>Første bolk være åpen?</label>
            <items>
                <input name="open" type="CheckBox">
                    <label>Skal første bolk være åpen?</label>
                    <custom-text>contentdata/open</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <item-set name="listitem">
            <label>utlisting</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="checklist" type="ContentSelector">
                    <label>Sjekkliste/Retningslinje</label>
                    <custom-text>contentdata/checklist</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Kvalsys_sjekkliste_NY</allow-content-type>
                        <allow-content-type>no.nav.navno:Kvalsys_retningslinje_NY</allow-content-type>
                    </config>
                </input>
            </items>
        </item-set>
    </form>
</content-type>
