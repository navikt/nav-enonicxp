<content-type>
    <display-name>Kvalsys_aktivitet_NY</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Oppdaterer du på vegne av noene andre?">
            <label>Oppdaterer du på vegne av noene andre?</label>
            <items>
                <input name="updated-by" type="TextLine">
                    <label>Oppdatert på vegne av</label>
                    <custom-text>contentdata/updated-by</custom-text>
                    <help-text>La feltet være tomt med mindre du redigerer innholdet på vegne av noen andre.</help-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="Aktivitet">
            <label>Aktivitet</label>
            <items>
                <input name="responsible" type="ComboBox">
                    <label>Dette utføres av</label>
                    <custom-text>contentdata/responsible</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                    <config>
                        <option value="Arbeidsgiver">Arbeidsgiver</option>
                        <option value="Bruker">Bruker</option>
                        <option value="Flere-aktører">Flere aktører</option>
                        <option value="NAV-kontor">NAV-kontor</option>
                    </config>
                </input>
                <input name="number" type="TextLine">
                    <label>Nummer på tilhørende element</label>
                    <custom-text>contentdata/number</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="heading" type="TextLine">
                    <label>Navn og nummer på tilhørende element</label>
                    <custom-text>contentdata/heading</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="prosess" type="CheckBox">
                    <label>Er dette en prosess?</label>
                    <custom-text>contentdata/prosess</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <item-set name="sections">
            <label>Retningslinje (brødtekst i flere seksjoner)</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="sectionheading" type="TextLine">
                    <label>Overskrift (h1)</label>
                    <custom-text>sectionheading</custom-text>
                    <help-text/>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="section" type="HtmlArea">
                    <label>Brødtekst (avsnitt)</label>
                    <custom-text>section</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </item-set>
        <field-set name="Utlistingtitler">
            <label>Utlistingtitler</label>
            <items>
                <input name="utlisting" type="ContentSelector">
                    <label>Utlisting av titler</label>
                    <custom-text>contentdata/utlisting</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Kvalsys_utlistingtitler_NY</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <field-set name="Støttemateriale">
            <label>Støttemateriale</label>
            <items>
                <input name="resources" type="ContentSelector">
                    <label>Lenker og systemstøtte</label>
                    <custom-text>contentdata/resources</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Kvalsys_aktivitet_NY</allow-content-type>
                        <allow-content-type>no.nav.navno:Kvalsys_sjekkliste_NY</allow-content-type>
                        <allow-content-type>no.nav.navno:Kvalsys_retningslinje_NY</allow-content-type>
                        <allow-content-type>no.nav.navno:Kvalsys_systemstotte_NY</allow-content-type>
                        <allow-content-type>no.nav.navno:Ekstern_lenke</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <field-set name="Elementer">
            <label>Elementer</label>
            <items>
                <input name="activities" type="ContentSelector">
                    <label>Elementer</label>
                    <custom-text>contentdata/activities</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Kvalsys_aktivitet_NY</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <field-set name="Sjekkliste">
            <label>Sjekkliste</label>
            <items>
                <input name="checklist" type="ContentSelector">
                    <label>Sjekkliste</label>
                    <custom-text>contentdata/checklist</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Kvalsys_sjekkliste_NY</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <field-set name="Retningslinje">
            <label>Retningslinje</label>
            <items>
                <input name="retningslinje" type="ContentSelector">
                    <label>Retningslinje</label>
                    <custom-text>contentdata/retningslinje</custom-text>
                    <occurrences maximum="0" minimum="0"/>
                    <config>
                        <allow-content-type>no.nav.navno:Kvalsys_retningslinje_NY</allow-content-type>
                    </config>
                </input>
            </items>
        </field-set>
        <item-set name="changelog">
            <label>Endringslogg</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="date" type="Date">
                    <label>Dato</label>
                    <custom-text>date</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="log" type="TextArea">
                    <label>Endring</label>
                    <custom-text>log</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="author" type="TextLine">
                    <label>Ansvarlig</label>
                    <custom-text>author</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="viktig" type="CheckBox">
                    <label>Er dette en viktig endringslogg?</label>
                    <custom-text>contentdata/viktig</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </item-set>
    </form>
</content-type>
