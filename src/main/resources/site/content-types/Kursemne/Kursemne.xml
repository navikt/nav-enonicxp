    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Kursemne">
            <label>Kursemne</label>
            <items>
                <input name="kursemne" type="TextLine">
                    <label>Kursemne</label>
                    <custom-text>contentdata/kursemne</custom-text>
                    <occurrences maximum="1" minimum="1"/>
