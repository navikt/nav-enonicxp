<content-type>
    <display-name>Selvbetjening_innloggingslinje</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Tjeneste">
            <label>Tjeneste</label>
            <items>
                <input name="environment" type="TextLine">
                    <label>Milj√∏</label>
                    <custom-text>contentdata/environment</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="authservice" type="TextLine">
                    <label>Autentiseringstjeneste</label>
                    <custom-text>contentdata/authservice</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="logouturl" type="TextLine">
                    <label>URL for utlogging</label>
                    <custom-text>contentdata/logouturl</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </field-set>
        <item-set name="authenticatedurls">
            <label>Autentiserte url'er</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="publicurl" type="TextLine">
                    <label>Public URL (regexp)</label>
                    <custom-text>publicurl</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="authenticatedurl" type="TextLine">
                    <label>Autentisert URL</label>
                    <custom-text>authenticatedurl</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
            </items>
        </item-set>
    </form>
</content-type>
