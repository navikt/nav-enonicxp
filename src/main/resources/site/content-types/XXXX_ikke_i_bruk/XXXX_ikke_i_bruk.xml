<content-type>
    <display-name>XXXX_ikke_i_bruk</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="Tilbakemelding">
            <label>Tilbakemelding</label>
            <items>
                <input name="to" type="TextLine">
                    <label>Kategori</label>
                    <custom-text>contentdata/to</custom-text>
                    <occurrences maximum="1" minimum="1"/>
                </input>
                <input name="taFeedback" type="TextArea">
                    <label>Ingress</label>
                    <custom-text>contentdata/taFeedback</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
                <input name="cc" type="TextLine">
                    <label>Email</label>
                    <custom-text>contentdata/cc</custom-text>
                    <occurrences maximum="1" minimum="0"/>
                </input>
            </items>
        </field-set>
    </form>
</content-type>
