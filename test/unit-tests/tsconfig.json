{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "resolveJsonModule": true,
        "target": "es2022",
        "module": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "noEmit": true,
        "baseUrl": "../../",
        "types": ["@types/jest"],
        "paths": {
            "@navno-app/*": ["src/main/resources/*"],
            "@xp-types/*": [".xp-codegen/*"],
            "/lib/cache": ["node_modules/@item-enonic-types/lib-cache"],
            "/lib/graphql": ["node_modules/@item-enonic-types/lib-graphql"],
            "/lib/graphql-playground": ["node_modules/@item-enonic-types/lib-graphql-playground"],
            "/lib/guillotine": ["node_modules/@item-enonic-types/lib-guillotine"],
            "/lib/http-client": ["node_modules/@item-enonic-types/lib-http-client"],
            "/lib/thymeleaf": ["node_modules/@item-enonic-types/lib-thymeleaf"],
            "/lib/xp/auditlog": ["node_modules/@enonic-types/lib-auditlog"],
            "/lib/xp/auth": ["node_modules/@enonic-types/lib-auth"],
            "/lib/xp/cluster": ["node_modules/@enonic-types/lib-cluster"],
            "/lib/xp/common": ["node_modules/@enonic-types/lib-common"],
            "/lib/xp/content": ["src/main/resources/types/xp-libs/lib-content"],
            "/lib/xp/context": ["node_modules/@enonic-types/lib-context"],
            "/lib/xp/event": ["node_modules/@enonic-types/lib-event"],
            "/lib/xp/grid": ["node_modules/@enonic-types/lib-grid"],
            "/lib/xp/io": ["node_modules/@enonic-types/lib-io"],
            "/lib/xp/node": ["src/main/resources/types/xp-libs/lib-node"],
            "/lib/xp/portal": ["src/main/resources/types/xp-libs/lib-portal"],
            "/lib/xp/project": ["node_modules/@enonic-types/lib-project"],
            "/lib/xp/repo": ["node_modules/@enonic-types/lib-repo"],
            "/lib/xp/scheduler": ["node_modules/@enonic-types/lib-scheduler"],
            "/lib/xp/task": ["node_modules/@enonic-types/lib-task"],
            "/lib/xp/value": ["node_modules/@enonic-types/lib-value"],
            "/lib/xp/websocket": ["node_modules/@enonic-types/lib-websocket"],
            "/assets/html-entities/2.3.3/lib": ["node_modules/html-entities"],
            "/assets/striptags/3.1.1/src/striptags": ["node_modules/striptags"],
            "/assets/dayjs/1.11.9/dayjs.min.js": ["node_modules/dayjs"],
            "/assets/dayjs/1.11.9/plugin/utc.js": ["node_modules/dayjs/plugin/utc"]
        }
    },
    "include": ["./**/*.ts", "../../src/main/resources/**/*.ts", "./tsconfig.json"]
}
